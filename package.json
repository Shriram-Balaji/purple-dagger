{
  "name": "rad-proton",
  "version": "1.0.0",
  "description": "The React Frontend to create a space for dev-news, dev-tips, daily design inspiration from across the web.",
  "main": "app/src/index.js",
  "scripts": {
    "build": "webpack",
    "start": "webpack-dev-server"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      [
        "babel-plugin-root-import",
        {
          "rootPathSuffix": "app/src/"
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shriram-Balaji/rad-proton.git"
  },
  "author": "Shriram Balaji",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shriram-Balaji/rad-proton/issues"
  },
  "homepage": "https://github.com/Shriram-Balaji/rad-proton#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-root-import": "^5.1.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "html-webpack-plugin": "^2.30.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  },
  "dependencies": {
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.6",
    "react-router-dom": "^4.2.2",
    "spectre.css": "^0.5.0",
    "style-loader": "^0.19.1",
    "url-loader": "^0.6.2"
  }
